<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>
      <dict>
	<key>cell</key>
	<string>PSGroupCell</string>
      <key>footerText</key>
      <string>You can change the emojis for existing words or add new ones. Make sure there is a comma between each emoji you would like to have.

Save "delete" in the emoji box to delete a word.

Changes require a respring.</string>
</dict>
<dict>
	<key>cell</key>  		<string>PSLinkListCell</string>
	<key>id</key>                     <string>editLanguage</string>
	<key>defaults</key> 	<string>com.fpt.emojiprefs</string>
	<key>detail</key> 		<string>PSListItemsController</string>
	<key>key</key> 			<string>editLanguage</string>
	<key>label</key> 		<string>Choose language</string>
	<key>validTitles</key> <array>
		<string>English</string>
		<string>German</string>
		<string>Dutch</string>
		<string>Spanish</string>
		<string>Portuguese</string>
		<string>Korean</string>
		<string>Italian</string>
            <string>Turkish</string>
	      <string>Hebrew</string>
	</array>
	<key>validValues</key>
	<array>
		<string>en</string>
		<string>de</string>
		<string>nl</string>
		<string>es</string>
		<string>pt</string>
		<string>ko</string>
		<string>it</string>
            <string>tu</string>
	      <string>he</string>
	</array>
	<key>PostNotification</key>  <string>com.fpt.emojiprefs/reloadPrefs</string>
</dict>
<dict>
     <key>cell</key>                   <string>PSEditTextCell</string>
		 <key>id</key>                     <string>inputWord</string>
		 <key>key</key>                    <string>inputWord</string>
		 <key>label</key>                  <string>Word:</string>
     <key>placeholder</key>            <string>Happy</string>
		 <key>defaults</key> 	             <string>com.fpt.emojiprefs</string>
		 <key>PostNotification</key>       <string>com.fpt.emojiprefs/reloadPrefs</string>
</dict>
<dict>
	<key>cell</key>                 <string>PSButtonCell</string>
			 <key>label</key>           <string>Load Emojis</string>
	<key>action</key>           <string>loadEmojis</string>
</dict>
<dict>
     <key>cell</key>                   <string>PSEditTextCell</string>
		 <key>id</key>                     <string>resultWord</string>
		 <key>key</key>                    <string>resultWord</string>
		 <key>label</key>                  <string>Emojis:</string>
     <key>placeholder</key>            <string></string>
		 <key>defaults</key> 	             <string>com.fpt.emojiprefs</string>
		 <key>PostNotification</key>       <string>com.fpt.emojiprefs/reloadPrefs</string>
</dict>
<dict>
	<key>cell</key>                 <string>PSButtonCell</string>
			 <key>label</key>           <string>Save Changes</string>
	<key>action</key>           <string>saveEmojis</string>
</dict>
	</array>
    <key>title</key>
    <string>Edit dictionaries</string>
</dict>
</plist>
