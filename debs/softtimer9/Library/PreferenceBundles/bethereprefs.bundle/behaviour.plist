<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>
		<dict>
				<key>cell</key>             <string>PSGroupCell</string>
				<key>label</key>            <string></string>
			<key>footerText</key>       <string>The timer will show how much time has passed since it fired. It will disappear when the phone is unlocked.
</string>
	</dict>

	<dict>
		<key>cell</key>	                <string>PSSwitchCell</string>
		<key>cellClass</key>            <string>BTSwitchTableCell</string>
		<key>default</key>              <true/>
		<key>defaults</key>             <string>com.fpt.bethereprefs</string>
		<key>key</key>                  <string>negativeTimer</string>
		<key>label</key>                <string>Show negative time</string>
	  <key>PostNotification</key>     <string>com.fpt.bethereprefs/reloadPrefs</string>
	</dict>

	<dict>
			<key>cell</key>             <string>PSGroupCell</string>
			<key>footerText</key>       <string>If the timer does not begin automatically, it can be started by tapping the underlined text.
</string>
	</dict>

	<dict>
		<key>cell</key>	                <string>PSSwitchCell</string>
		<key>cellClass</key>            <string>BTSwitchTableCell</string>
		<key>default</key>              <true/>
		<key>defaults</key>             <string>com.fpt.bethereprefs</string>
		<key>key</key>                  <string>autoTimer</string>
		<key>label</key>                <string>Automatically start timer</string>
	  <key>PostNotification</key>     <string>com.fpt.bethereprefs/reloadPrefs</string>
	</dict>

	<dict>
			<key>cell</key>             <string>PSGroupCell</string>
			<key>label</key>       <string>Vibration settings</string>
			<key>footerText</key>       <string>Vibration applies to both setting a timer and when it finishes. Devices from iPhone 6s upwards will use the taptic engine for light and normal options.</string>

	</dict>

	<dict>
		<key>cell</key>	                <string>PSSwitchCell</string>
		<key>cellClass</key>            <string>BTSwitchTableCell</string>
		<key>default</key>              <false/>
		<key>defaults</key>             <string>com.fpt.bethereprefs</string>
		<key>key</key>                  <string>autoVibrate</string>
		<key>label</key>                <string>Vibrate when setting a timer</string>
	  <key>PostNotification</key>     <string>com.fpt.bethereprefs/reloadPrefs</string>
	</dict>

		<dict>
			<key>cell</key>  		<string>PSLinkListCell</string>
			<key>defaults</key> 	<string>com.fpt.bethereprefs</string>
			<key>detail</key> 		<string>PSListItemsController</string>
			<key>key</key> 			<string>vibration</string>
			<key>label</key> 		<string>Vibration</string>
			<key>validTitles</key> <array>
				<string>Light</string>
				<string>Normal</string>
				<string>Heartbeat (default)</string>
				<string>Long</string>
			</array>
			<key>validValues</key>
			<array>
				<string>0</string>
				<string>1</string>
				<string>2</string>
				<string>3</string>
			</array>
			<key>PostNotification</key>  <string>com.fpt.bethereprefs/reloadPrefs</string>
		</dict>

	<dict>
			<key>cell</key>             <string>PSGroupCell</string>
	</dict>

		<dict>
					<key>cell</key>                  <string>PSLinkCell</string>
					<key>label</key>                <string>Advanced options</string>
					<key>detail</key>               <string>advancedController</string>
		</dict>

	</array>
	<key>title</key>
	<string>Behaviour</string>
</dict>
</plist>
